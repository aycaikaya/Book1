public static void main(String[] args) {
        int[] a={1,2,3,4};
        System.out.println(Histogram(a,5));
       
        
    }
    public static int[] Histogram(int[] array, int M){   
        int[] b = new int[M];
        for(int i=0;i<array.length;i++){
            int index=array[i];
            if(index<b.length){
                b[index]++;
            }
        }
        for(int k=0;k<b.length;k++){
            System.out.println(b[k]+" ");
        }
        return b;
       
 
}
